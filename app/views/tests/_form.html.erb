<%= form_for(@test) do |f| %>
  <div class="field">
    <%= f.label :test_category_name %><br>
    <%= f.text_field :test_category_name %>
  </div>

  <div class="field">
    <%= f.fields_for :questions do |builder| %>
    <p>
      <%= render 'question_fields', :f => builder %>
    </p>
    <% end %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
 
